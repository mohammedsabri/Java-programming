<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="377.0" prefWidth="393.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainController">
   <children>
      <VBox layoutX="75.0" layoutY="35.0" prefHeight="307.0" prefWidth="244.0">
         <children>
            <Button fx:id="handleViewPropertiesBtn" mnemonicParsing="false" onAction="#handleViewPropertiesBtn" prefHeight="67.0" prefWidth="248.0" text="View Properties" textFill="#0813e4">
               <font>
                  <Font name="System Bold Italic" size="24.0" />
               </font>
            </Button>
            <Button fx:id="handleLoginAdminBtn" mnemonicParsing="false" onAction="#handleLoginAdminBtn" prefHeight="67.0" prefWidth="250.0" text="Login as Admin" textFill="#0813e4">
               <font>
                  <Font name="System Bold Italic" size="24.0" />
               </font>
            </Button>
            <Button fx:id="handleLoginAgentBtn" mnemonicParsing="false" onAction="#handleLoginAgentBtn" prefHeight="58.0" prefWidth="245.0" text="Login as Agent" textFill="#0813e4">
               <font>
                  <Font name="System Bold Italic" size="24.0" />
               </font>
            </Button>
            <Button fx:id="handleRegisterBtn" mnemonicParsing="false" onAction="#handleRegisterBtn" prefHeight="53.0" prefWidth="254.0" text="Register Agent" textFill="#0813e4">
               <font>
                  <Font name="System Bold Italic" size="24.0" />
               </font>
            </Button>
            <Button fx:id="handleRegisterBtn1" mnemonicParsing="false" onAction="#handleRegisterAdmin" prefHeight="56.0" prefWidth="252.0" text="Register Admin" textFill="#0813e4">
               <font>
                  <Font name="System Bold Italic" size="24.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
