<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RegisterController">
   <children>
      <Label layoutX="182.0" layoutY="14.0" prefHeight="53.0" prefWidth="306.0" text="Register Agent">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <Label layoutX="22.0" layoutY="92.0" prefHeight="27.0" prefWidth="145.0" text="Enter Username:" textFill="#030111">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="22.0" layoutY="142.0" prefHeight="27.0" prefWidth="145.0" text="Enter Password:" textFill="#02000d">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="7.0" layoutY="195.0" prefHeight="27.0" prefWidth="166.0" text="Re-Enter Password:" textFill="#030111">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button layoutX="141.0" layoutY="326.0" mnemonicParsing="false" onAction="#handleHomeBtn" prefHeight="51.0" prefWidth="145.0" text="Home">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Button>
      <Button layoutX="369.0" layoutY="326.0" mnemonicParsing="false" onAction="#handleRegisterBtn" prefHeight="51.0" prefWidth="139.0" text="Register">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Button>
      <TextField fx:id="username" layoutX="182.0" layoutY="90.0" prefHeight="30.0" prefWidth="373.0" promptText="Enter Username">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <PasswordField fx:id="password" layoutX="182.0" layoutY="141.0" prefHeight="30.0" prefWidth="373.0" promptText="Enter Password">
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="repeat_password" layoutX="183.0" layoutY="193.0" prefHeight="30.0" prefWidth="373.0" promptText="Re-Enter Password">
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
      <Label fx:id="txtAreaFeedback" layoutX="20.0" layoutY="230.0" prefHeight="20.0" prefWidth="260.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
