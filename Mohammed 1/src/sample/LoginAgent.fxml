<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginAgentController">
   <children>
      <Label layoutX="204.0" prefHeight="53.0" prefWidth="323.0" text="Login Agent" textFill="#2309d0">
         <font>
            <Font name="System Bold Italic" size="36.0" />
         </font>
      </Label>
      <Label layoutX="23.0" layoutY="85.0" text="Username" textFill="#2309d0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="25.0" layoutY="131.0" prefHeight="27.0" prefWidth="103.0" text="Password" textFill="#2309d0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="140.0" layoutY="86.0" promptText="username" />
      <PasswordField fx:id="password" layoutX="140.0" layoutY="132.0" promptText="Password" />
      <TextArea fx:id="txtAreaFeedback" layoutX="14.0" layoutY="229.0" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="handleLoginBtn" layoutX="397.0" layoutY="354.0" mnemonicParsing="false" onAction="#handleLoginBtn" prefHeight="51.0" prefWidth="120.0" text="Login" textFill="#2309d0">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Button>
      <Button fx:id="handleHomeBtn" layoutX="231.0" layoutY="354.0" mnemonicParsing="false" onAction="#handleHomeBtn" prefHeight="51.0" prefWidth="120.0" text="Home" textFill="#2309d0">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
